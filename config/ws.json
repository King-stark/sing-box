    "dns": {
        "servers": [
            {
                "tag": "google-tls",
                "address": "tls://dns.google",
                "address_resolver": "local",
                "address_strategy": "prefer_ipv4",
                "strategy": "prefer_ipv4",
                "detour": "direct"
            },
            {
                "tag": "google-udp",
                "address": "8.8.8.8",
                "address_strategy": "prefer_ipv4",
                "strategy": "prefer_ipv4",
                "detour": "direct"
            },
            {
                "tag": "local",
                "address": "local",
                "detour": "direct"
            }
        ],
        "strategy": "prefer_ipv4",
        "disable_cache": false,
        "disable_expire": false
    },

  "inbounds": [
